<view class="all">
    <view class="login" style="height:200px;" bindtap="login">
        <image src="data:image/gif;base64,"
            style="height: 200px" class="loginbg"></image>
        <view wx:if="{{userInfo}}">
            <image lazy-load src="{{userInfo.avatarUrl}}" style="border-radius: 100px;border:2px solid #fff" class="personal_logo" id="personal_logo"></image>
            <view class="userinfo">
                <view class="title" id="user_name">
                    {{userInfo.userName?userInfo.userName:(userInfo.nickName?userInfo.nickName:"点击授权登录")}}
                </view>
                <view class="subtitle" id="shenfen" wx:if="{{certificationOk == 0}}">
                    未认证，点击前往认证
                </view>
                <view class="subtitle" id="shenfen" wx:elif="{{certificationOk == 1}}">
                    认证中
                </view>
                <view class="subtitle" id="shenfen" wx:elif="{{certificationOk == 2}}">
                    已认证
                </view>
                <view class="subtitle" id="shenfen" wx:elif="{{certificationOk == 3}}">
                    被驳回
                </view>
                <view class="subtitle" id="shenfen" wx:else>
                    {{certificationOk_notice}}
                </view>
            </view>
        </view>
        <view wx:else style='position:fixed;top:20px;left:10%;width:80%;margin:0 auto'>
            <view style='width:100%;'><image src='../../images/selfCenterPhoto.png' style='width:90px;height:90px;margin:0 auto;display:block'></image></view>
            <button wx:if="{{canIUse}}" plain='true' open-type="getUserInfo" bindgetuserinfo="bindGetUserInfo" style='border:0px solid #fff;color:#fff'>点击授权登录</button> 
        </view>
    </view>
    <!-- 第一块 -->
    <view class="thirdblock">
        <view class="h1"></view>
        <view class="item" bindtap="openMyCollect" data-index="2">
            <image src="../../images/booklist.png" class="item_ico"></image>
            <span>我的收藏</span>
            <!-- <image src="../../images/right.png" class="item_arrow"></image> -->
        </view>
        <view class="h1"></view>
        <view class="item" bindtap="openMyHouse" data-index="2">
            <image src="../../images/booklist.png" class="item_ico"></image>
            <span>我的房屋</span>
        </view>
        <view class="h1"></view>
    </view>
    <!-- 第三块 -->
    <view class="h10"></view>
    <view class="thirdblock">
        <view class="item" bindtap="openOpinion" data-index="5" bindtap="openOpinion">
            <image src="../../images/suggest.png" class="item_ico"></image>
            <span>意见反馈</span>
            <!-- <image src="../../images/right.png" class="item_arrow"></image> -->
        </view>
        <view class="h1"></view>
        <!-- <view class="item" bindtap="aboutUs" data-index="4">
            <image src="../../images/about.png" class="item_ico"></image>
            <span>关于我们</span>
        </view>
        <view class="h1"></view>
        <view class="item" bindtap="versionUpdate">
            <image src="../../images/setting.png" class="item_ico"></image>
            <span>更新小助手</span>
        </view> -->
    </view>
    <view class="h10"></view>
    <view class="h10"></view>
</view>